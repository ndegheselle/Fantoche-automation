<UserControl
    x:Class="Automation.App.Views.WorkPages.Workflows.Editor.GraphEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Automation.App.Views.WorkPages.Workflows.Editor"
    mc:Ignorable="d"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:Name="ContainerElement">
    <Grid DataContext="{Binding ElementName=ContainerElement}">
        <local:GraphEditorCanvas x:Name="Canvas" />
        <local:GraphEditorOverlay Editor="{Binding Editor}" />
    </Grid>
</UserControl>
