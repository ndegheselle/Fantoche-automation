<UserControl
    x:Class="Automation.App.Views.WorkflowUI.WorkflowPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Automation.App.Views.WorkflowUI"
    mc:Ignorable="d"
    xmlns:base="clr-namespace:Automation.Base;assembly=Automation.Base"
    d:DataContext="{d:DesignInstance base:WorkflowScope}">
    <local:WorkflowGraph WorkflowScope="{Binding}" />
</UserControl>
