<UserControl
    x:Class="Automation.App.Views.WorkflowUI.WorkflowPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Automation.App.Views.WorkflowUI"
    mc:Ignorable="d"
    d:DataContext="{d:DesignInstance local:WorkflowPage}">

    <TabControl>
        <TabItem Header="Parameters" />
        <TabItem Header="Graph">
            <local:WorkflowGraph EditorData="{Binding Editor, Mode=OneTime}" />
        </TabItem>
    </TabControl>
</UserControl>
